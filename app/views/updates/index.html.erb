<h1>Listing update</h1>

  <table>
    <tr>
      <th>Title</th>
      <th>Text</th>
      <th></th>
    </tr>

    <% @updates.each do |update| %>
      <tr>
        <td><%= update.version %></td>
        <td><%= update.description %></td>
        <td><%= link_to 'Show', update_path(update) %></td>
        <td><%= link_to 'Edit', edit_update_path(update) %></td>
        <td><%= link_to 'Destroy', update_path(update),
                method: :delete,
                data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </table>

<% if current_admin %>
  <%= link_to 'admins back', admin_path(current_admin)%>
<% end %>

<p id=""><a href ="/">HOME</a></p>
